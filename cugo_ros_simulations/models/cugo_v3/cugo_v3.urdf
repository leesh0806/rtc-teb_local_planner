      <inertia ixx="0.00031028364375" ixy="0" ixz="0" iyy="0.00031028364375" iyz="0" izz="0.00038791124999999993"/>
    </inertial>
    <visual>
      <material name="ouster_grey">
        <color rgba="0.8 0.8 0.8 1.0"/>
      </material>
      <origin xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://clearpath_sensors_description/meshes/ouster/os1_lidar.dae"/>
      </geometry>
    </visual>
    <collision>
      <material>
        <color rgba="0.8 0.8 0.8 1.0"/>
      </material>
      <origin rpy="0 0 0" xyz="0 0 0.029175"/>
      <geometry>
        <cylinder length="0.05835" radius="0.0435"/>
      </geometry>
    </collision>
  </link>
  <link name="ouster_cap_link">
    <inertial>
      <mass value="0.092"/>
      <origin xyz="0 0 0.011"/>
      <inertia ixx="4.7022565241666676e-05" ixy="0" ixz="0" iyy="4.7022565241666676e-05" iyz="0" izz="8.662379715000001e-05"/>
    </inertial>
    <visual>
      <material name="ouster_grey">
        <color rgba="0.8 0.8 0.8 1.0"/>
      </material>
      <origin xyz="0 0 -0.05835"/>
      <geometry>
        <mesh filename="package://clearpath_sensors_description/meshes/ouster/os1_halo.dae"/>
      </geometry>
    </visual>
    <collision>
      <material>
        <color rgba="0.8 0.8 0.8 1.0"/>
      </material>
      <origin rpy="0 0 0" xyz="0 0 0.011"/>
      <geometry>
        <cylinder length="0.022" radius="0.043395"/>
      </geometry>
    </collision>
  </link>
  <!-- Base Joint -->
  <joint name="ouster_base_joint" type="fixed">
    <child link="ouster_base_link"/>
    <parent link="base_link"/>
    <origin rpy="0 0 0" xyz="0.0 0.0 0.15"/>
  </joint>
  <!-- Lidar Joint -->
  <joint name="ouster_joint" type="fixed">
    <child link="ouster_link"/>
    <parent link="ouster_base_link"/>
    <origin xyz="0 0 0.0225"/>
  </joint>
  <!-- Thermal Cap Joint -->
  <joint name="{name}_cap_joint" type="fixed">
    <child link="ouster_cap_link"/>
    <parent link="ouster_link"/>
    <origin xyz="0 0 0.05835"/>
  </joint>
  <!-- Sensor Joint -->
  <link name="ouster_sensor_link"/>
  <joint name="ouster_sensor_joint" type="fixed">
    <child link="ouster_sensor_link"/>
    <parent link="ouster_link"/>
    <origin xyz="0 0 0.038195"/>
  </joint>
  <!-- Gazebo -->
  <gazebo reference="ouster_sensor_link">
    <sensor name="ouster" type="gpu_lidar">
      <update_rate>10</update_rate>
      <visualize>true</visualize>
      <always_on>true</always_on>
      <ignition_frame_id>ouster_sensor_link</ignition_frame_id>
      <topic>/sensors/ouster/scan</topic>
      <lidar>
        <scan>
          <horizontal>
            <samples>720</samples>
            <resolution>1</resolution>
            <min_angle>-3.141592653589793</min_angle>
            <max_angle>3.141592653589793</max_angle>
          </horizontal>
          <vertical>
            <samples>32</samples>
            <resolution>1</resolution>
            <min_angle>-0.2897246558310587</min_angle>
            <max_angle>0.2897246558310587</max_angle>
          </vertical>
        </scan>
        <range>
          <min>0.5</min>
          <max>120.0</max>
          <resolution>0.01</resolution>
        </range>
      </lidar>
    </sensor>
  </gazebo>
  <!-- ========== IMU Sensor ========== -->
  <!-- IMU joint (fixed to base_link) -->
  <joint name="imu_joint" type="fixed">
    <parent link="base_link"/>
    <child link="imu_link"/>
    <!-- Position: centered on robot body, 3cm above base_link (lowered for stability) -->
    <origin rpy="0 0 0" xyz="0.0 0.0 0.03"/>
  </joint>
  <!-- IMU link with visual representation -->
  <link name="imu_link">
    <visual>
      <geometry>
        <!-- Small box representing IMU sensor -->
        <box size="0.03 0.03 0.02"/>
      </geometry>
      <material name="dark"/>
    </visual>
    <collision>
      <geometry>
        <!-- Small box representing IMU sensor -->
        <box size="0.03 0.03 0.02"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.05"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="1e-4" ixy="0" ixz="0" iyy="1e-4" iyz="0" izz="1e-4"/>
    </inertial>
  </link>
  <!-- ========== Ignition Gazebo IMU Sensor ========== -->
  <!--
  IMU sensor provides:
  - Linear acceleration (with gravity)
  - Angular velocity (gyroscope)
  - Orientation (computed from accelerometer + gyroscope)
  
  NOTE: This follows the same pattern as the LiDAR sensor above
  No separate plugin tag needed - Ignition handles it internally
  -->
  <gazebo reference="imu_link">
    <sensor name="imu_sensor" type="imu">
      <!-- Sensor pose relative to parent link -->
      <pose>0 0 0 0 0 0</pose>
      <!-- ROS topic name for IMU messages -->
      <topic>imu</topic>
      <!-- Update rate in Hz -->
      <update_rate>100</update_rate>
      <!-- Keep sensor always active -->
      <always_on>1</always_on>
      <!-- Visualize IMU orientation in Gazebo GUI -->
      <visualize>true</visualize>
      <!-- IMU configuration -->
      <imu>
        <!-- Enable orientation output -->
        <orientation_enabled>true</orientation_enabled>
        <!-- Angular velocity (gyroscope) noise parameters -->
        <angular_velocity>
          <x>
            <noise type="gaussian">
              <mean>0.0</mean>
              <stddev>2e-4</stddev>
              <bias_mean>0.0000075</bias_mean>
              <bias_stddev>0.0000008</bias_stddev>
            </noise>
          </x>
          <y>
            <noise type="gaussian">
              <mean>0.0</mean>
              <stddev>2e-4</stddev>
              <bias_mean>0.0000075</bias_mean>
              <bias_stddev>0.0000008</bias_stddev>
            </noise>
          </y>
          <z>
            <noise type="gaussian">
              <mean>0.0</mean>
              <stddev>2e-4</stddev>
              <bias_mean>0.0000075</bias_mean>
              <bias_stddev>0.0000008</bias_stddev>
            </noise>
          </z>
        </angular_velocity>
        <!-- Linear acceleration (accelerometer) noise parameters -->
        <linear_acceleration>
          <x>
            <noise type="gaussian">
              <mean>0.0</mean>
              <stddev>1.7e-2</stddev>
              <bias_mean>0.1</bias_mean>
              <bias_stddev>0.001</bias_stddev>
            </noise>
          </x>
          <y>
            <noise type="gaussian">
              <mean>0.0</mean>
              <stddev>1.7e-2</stddev>
              <bias_mean>0.1</bias_mean>
              <bias_stddev>0.001</bias_stddev>
            </noise>
          </y>
          <z>
            <noise type="gaussian">
              <mean>0.0</mean>
              <stddev>1.7e-2</stddev>
              <bias_mean>0.1</bias_mean>
              <bias_stddev>0.001</bias_stddev>
            </noise>
          </z>
        </linear_acceleration>
      </imu>
      <!-- Frame ID for TF transformations -->
      <!-- This must match the link name for proper TF tree -->
      <ignition_frame_id>imu_link</ignition_frame_id>
    </sensor>
  </gazebo>
</robot>
